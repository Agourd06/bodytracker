{
  "expo": {
    "name": "BodyTracker",
    "slug": "bodytracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oualidagourd.bodytracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oualidagourd.bodytracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permettre à $(PRODUCT_NAME) d'accéder à votre caméra"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permettre à $(PRODUCT_NAME) d'accéder à vos photos",
          "savePhotosPermission": "Permettre à $(PRODUCT_NAME) de sauvegarder des photos",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2cf834d4-7641-488a-a565-7bc20b7350df"
      }
    },
    "owner": "oualidagourd",
    "updates": {
      "url": "https://u.expo.dev/2cf834d4-7641-488a-a565-7bc20b7350df"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
